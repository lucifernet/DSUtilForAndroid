<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">

	<xs:element name="Header">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="TargetContract" type="xs:string" minOccurs="0"/>
			<xs:element name="TargetService" type="xs:string"/>
			<xs:element name="SecurityToken" minOccurs="1">
				<xs:complexType>
					<xs:choice>
						<xs:sequence>
							<xs:sequence minOccurs="1">
								<xs:element name="UserName" type="xs:string"/>
								<xs:element name="Password" type="xs:string"/>
							</xs:sequence>
							<xs:sequence minOccurs="0">
								<xs:element name="Timestamp" type="xs:dateTime"/>
								<xs:element name="SignatureValue" type="xs:string"/>
							</xs:sequence>
						</xs:sequence>
						<xs:sequence minOccurs="0">
							<xs:element name="DSAPassport">
								<xs:complexType>
									<xs:sequence>
										<xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip"/>
									</xs:sequence>
									<xs:attribute name="Version" type="xs:string"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:sequence minOccurs="0">
							<xs:element name="SessionID" type="xs:string"/>
						</xs:sequence>
					</xs:choice>
					<xs:attribute name="Type" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Basic"/>
								<xs:enumeration value="Enhanced"/>
								<xs:enumeration value="DSAPassport"/>
								<xs:enumeration value="Session"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip"/>
		</xs:sequence>
	</xs:complexType>

	</xs:element>
</xs:schema>